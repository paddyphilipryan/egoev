      <section class="inner-header bg-overlay-black-6 bg-holder" style="background-image: url({{ entry.featuredImage.one().getUrl('transform1') }});">
        <div class="container">
          <div class="row">
            <div class="col-lg-12">
              <div class="inner-header-tittle">
                <h2>{{ entry.title }}</h2>
                <ol class="breadcrumb">
                  <li class="breadcrumb-item"><a href="{{ siteUrl }}">Home</a></li>
                  <li class="breadcrumb-item active">{{ entry.title }}</li>
                  <li class="breadcrumb-item active"><span class="post-date"><i class="fa-solid fa-calendar-days"></i>  {{blog.postDate|date(' d/M/Y')}}</span></li>
                </ol>
              </div>
            </div>
          </div>
        </div>
      </section>
